
<div class="conatiner-fluid" style="padding-left: 20%; padding-top: 115px; padding-right: 20%; position: relative;">
    <% if(success && success.length) { %>
        <h1 class="alert alert-success" role="alert"><%= success %></h1>
    <% } %>

    <% if(error && error.length) { %>
        <h1 class="alert alert-danger" role="alert"><%= error %></h1>
    <% } %>
</div>